    "# Spark Notebook"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from pyspark.sql import SparkSession"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Connect to Spark 4.0.1 with Comet"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "spark_comet = SparkSession.builder \\",
    "    .appName(\"SparkComet\") \\",
    "    .master(\"spark://spark-master-4.0.1-comet:7077\") \\",
    "    .getOrCreate()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "spark_comet.sparkContext.setLogLevel("INFO")",
    "df_comet = spark_comet.createDataFrame([(i,) for i in range(10)], ["a"])",
    "df_comet.write.mode("overwrite").parquet("/data/test_comet")",
    "spark_comet.read.parquet("/data/test_comet").createOrReplaceTempView("t1_comet")",
    "spark_comet.sql("select * from t1_comet where a > 5").explain()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "spark_comet.stop()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Connect to Spark 4.1.0"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "spark_normal = SparkSession.builder \\",
    "    .appName(\"SparkNormal\") \\",
    "    .master(\"spark://spark-master-4.1.0:7078\") \\",
    "    .getOrCreate()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_normal = spark_normal.createDataFrame([(i,) for i in range(10)], ["a"])",
    "df_normal.write.mode("overwrite").parquet("/data/test_normal")",
    "spark_normal.read.parquet("/data/test_normal").createOrReplaceTempView("t1_normal")",
    "spark_normal.sql("select * from t1_normal where a > 5").explain()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "spark_normal.stop()"
   ]
  }
